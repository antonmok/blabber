{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "C/C++: aarch64-linux-gnu-g++-7 build active file",
			"command": "/usr/bin/aarch64-linux-gnu-g++-7",
			"args": [
				"-g",
				"${file}",
				"-I","/usr/include",
				"-I","/usr/local/include",
				//"-I","/mnt/m2/curl/curl/include",
				"-L","/usr/local/lib",
				"-L","/usr/lib/aarch64-linux-gnu",
				"-L","/mnt/m2/opencv/opencv/build/lib",
				"-l","opencv_highgui",
				"-l","opencv_imgproc",
				"-l","opencv_core",
				"-l","opencv_imgcodecs",
				"-l","opencv_videoio",
				"-l","opencv_cudaimgproc",
				"-l","opencv_cudaoptflow",
				"-l","opencv_tracking",
				"-l","opencv_video",
				"-l","opencv_xfeatures2d",
				"-l","opencv_features2d",
				"-I","/mnt/m2/opencv/opencv/modules/highgui/include/",
				"-I","/mnt/m2/opencv/opencv/modules/imgproc/include/",
				"-I","/mnt/m2/opencv/opencv/modules/imgcodecs/include/",
				"-I","/mnt/m2/opencv/opencv/modules/videoio/include/",
				"-I","/mnt/m2/opencv/opencv/include",
				"-I","/mnt/m2/opencv/opencv/build",
				"-I","/mnt/m2/opencv/opencv/modules/core/include",
				"-I","/mnt/m2/opencv/opencv/modules/calib3d/include/",
				"-I","/mnt/m2/opencv/opencv/modules/features2d/include/",
				"-I","/mnt/m2/opencv/opencv/modules/flann/include/",
				"-I","/mnt/m2/opencv/opencv/modules/dnn/include/",
				"-I","/mnt/m2/opencv/opencv/modules/ml/include/",
				"-I","/mnt/m2/opencv/opencv/modules/objdetect/include/",
				"-I","/mnt/m2/opencv/opencv/modules/photo/include/",
				"-I","/mnt/m2/opencv/opencv/modules/stitching/include/",
				"-I","/mnt/m2/opencv/opencv/modules/video/include/",
				"-I","/mnt/m2/opencv/opencv/opencv_contrib/modules/cudaarithm/include/",
				"-I","/mnt/m2/opencv/opencv/opencv_contrib/modules/cudaimgproc/include",
				"-I","/mnt/m2/opencv/opencv/opencv_contrib/modules/cudaoptflow/include",
				"-I","/usr/local/include/opencv4/",
                "-l","pthread",
                "-l","boost_system",
                "-l","boost_filesystem",
				"-l","boost_chrono",
				"-l","boost_thread",
				"-l","stdc++fs",
				"-l","pthread",
				"-l","jpeg",
				"-l","rt",
				"-l","websockets",
				"-o",
				"${fileDirname}/${fileBasenameNoExtension}"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		}
	]
}